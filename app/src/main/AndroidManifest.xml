            android:exported="false"
            android:theme="@style/blackActionBar" />
        <activity
            android:name="com.app.citycareservice.ui.activities.settings.ManageAddressActivity"
            android:exported="false"
            android:theme="@style/whiteActionBar" />
        <activity
            android:name="com.app.citycareservice.ui.activities.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.app.citycareservice.ui.activities.MainActivity"
            android:exported="true">

            <!-- <intent-filter> -->


            <!-- <action android:name="android.intent.action.MAIN" /> -->


            <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
            <!-- </intent-filter> -->
        </activity>
        <activity
            android:name="com.app.citycareservice.ui.activities.authentication.AddDetailActivity"
            android:exported="false"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name="com.app.citycareservice.ui.activities.service.SearchActivity"
            android:exported="false"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name="com.app.citycareservice.ui.activities.service.ServiceCategoryDetailActivity"
            android:exported="false"
            android:windowSoftInputMode="adjustResize" />
