            android:name="com.app.citycareservice.ui.activities.service.ServiceDetailActivity"
            android:exported="false"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name="com.app.citycareservice.ui.activities.authentication.LoginActivity"
            android:exported="true"
            android:windowSoftInputMode="adjustResize">

            <!-- <intent-filter> -->
            <!-- <action android:name="android.intent.action.MAIN" /> -->


            <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
            <!-- </intent-filter> -->
        </activity>

        <service
            android:name="com.app.citycareservice.service.UpdateProfileService"
            android:exported="false" />
